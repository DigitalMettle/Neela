{
  "ConnectionStrings": {
    "DefaultConnection": "{Database Connection String}"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AppSettings": {
    "JwtIssuerOptions": {
      "Issuer": "issuer",
      "Audience": "audience"
    },
    "VSTS": {
      "AccountName": "{your account name}",
      "ByPassRules": "true",
      "UserProfileUrl": "https://app.vssps.visualstudio.com/_apis/profile/profiles/me?api-version=4.1",
      "GroupListUrl": "https://vssps.dev.azure.com/{0}/_apis/graph/groups?api-version=4.1-preview.1",
      "GroupMembersUrl": "https://vsaex.dev.azure.com/{0}/_apis/GroupEntitlements/{1}/members?api-version=4.1-preview.1",
      "VSTSGroupName": "{vsts administrator group name}",
      "PersonalAccessToken": "{Personal Access Token}",
      "APIVersion": "4.1"
    },

    "AppSettingOauth": {
      "AppId": "{App Id}",
      "AppSecret": "{App Secret}",
      "Scope": "vso.work_write",
      "AuthUrl": "https://app.vssps.visualstudio.com/oauth2/authorize?mkt=en-US",
      "TokenUrl": "https://app.vssps.visualstudio.com/oauth2/token?mkt=en-US",
      "CallbackUrl": "{Your domain name}/oauth/callback"
    },

    "jwtAppSettingOptions": {
      "Issuer": "Issuer",
      "Audience": "Audience"
    },
    "FolderPath": {
      "Path": "UserImages",
      "ImagePath": "UserImage/UserImages",
      "RootFolder": "wwwroot",
      "CompanyImagePath": "CompanyImage"
    }
  }

}
